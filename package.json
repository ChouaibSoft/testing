  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf build dist",
    "clean-win": "rmdir build /S /Q & rmdir dist/S /Q",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "start-electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "build-electron-win": "robocopy electron build/electron /S",
    "package": "electron-builder build  --win -c.extraMetadata.main=build/electron/main.js  --publish always",
    "build-desktop": "yarn run clean-win && yarn run build && yarn run build-electron-win && yarn run package"
  },
